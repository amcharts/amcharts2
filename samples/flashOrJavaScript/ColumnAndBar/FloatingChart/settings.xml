<?xml version="1.0" encoding="UTF-8"?>
<settings>
  <type>bar</type>
  <plot_area>
    <margins>
      <left>70</left>
      <right>50</right>
      <top>50</top>
      <bottom>70</bottom>
    </margins>
  </plot_area>
  <grid>
    <category>
      <alpha>20</alpha>
      <dashed>1</dashed>
      <dash_length>4</dash_length>
    </category>
    <value>
      <alpha>20</alpha>
      <dashed>1</dashed>
      <dash_length>4</dash_length>
      <approx_count>15</approx_count>
    </value>
  </grid>
  <axes>
    <category>
      <width>1</width>
    </category>
    <value>
      <width>1</width>
    </value>
  </axes>
  <values>
    <value>
      <rotate>90</rotate>
      <unit>:00</unit>
    </value>
  </values>
  <legend>
    <enabled>0</enabled>
  </legend>
  <column>
    <width>100</width>
    <spacing>0</spacing>
    <alpha>60</alpha>
    <gradient>horizontal</gradient>
    <balloon_text><![CDATA[{series}s work hours: {start}:00 - {value}:00</b>]]></balloon_text>
    <data_labels_text_color>FFFFFF</data_labels_text_color>
    <data_labels_position>inside</data_labels_position>
    <grow_time>2</grow_time>
    <grow_effect>strong</grow_effect>
    <sequenced_grow>1</sequenced_grow>
  </column>
  <line>
    <balloon_text>{value}</balloon_text>
    <data_labels>{value}</data_labels>
  </line>
  <labels>
    <label lid="0">
      <text><![CDATA[<b>Work schedule</b>]]></text>
      <y>20</y>
      <text_size>14</text_size>
      <align>center</align>
    </label>
  </labels>
</settings>